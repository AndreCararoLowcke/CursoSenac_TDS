programa {
  funcao inicio() {
    // Parte 1 da atividade 1 UC2

    inteiro garcon=0, horas=0
    real precohora=10.5, custototal
    escreva ("Digite quantos garçons você quer:")
    leia (garcon)
    escreva ("Digite quantas horas o evento vai durar:")
    leia (horas)

    custototal = garcon*horas*precohora 

    escreva ("O custo total será:  ",custototal)
  }
}
